{
  "cli": {
    "version": ">= 9.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "ios": {
        "buildType": "release"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
